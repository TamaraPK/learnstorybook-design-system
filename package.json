{
  "name": "@tamarapk/learnstorybook-design-system",
  "version": "0.2.13",
  "private": false,
  "dependencies": {
    "@babel/runtime": "^7.12.13",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "polished": "^3.6.6",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.4",
    "styled-components": "^4.3.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "cross-env BABEL_ENV=production babel src -d dist",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "chromatic": "npx chromatic",
    "build-storybook-docs": "build-storybook -s public --docs",
    "release": "auto shipit --base-branch=main"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      [
        "@babel/plugin-transform-runtime",
        {
          "absoluteRuntime": false,
          "corejs": false,
          "helpers": true,
          "regenerator": true,
          "useESModules": false,
          "version": "7.0.0-beta.0"
        }
      ]
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.12.15",
    "@storybook/addon-a11y": "^6.1.17",
    "@storybook/addon-actions": "^6.1.17",
    "@storybook/addon-essentials": "^6.1.17",
    "@storybook/addon-links": "^6.1.17",
    "@storybook/node-logger": "^6.1.17",
    "@storybook/preset-create-react-app": "^3.1.5",
    "@storybook/react": "^6.1.17",
    "auto": "^10.16.0",
    "chromatic": "^5.6.1",
    "cross-env": "^7.0.3",
    "prettier": "^2.2.1"
  },
  "description": "Just learning how to create and produce a design system for use across products",
  "main": "dist/index.js",
  "repository": "git+https://github.com/TamaraPK/learnstorybook-design-system.git",
  "author": "TamaraPK",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TamaraPK/learnstorybook-design-system/issues"
  },
  "homepage": "https://github.com/TamaraPK/learnstorybook-design-system#readme"
}
